<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snapchatstartscreen">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.camera2.any" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="snapchat.example.snapchat.mainCamera"></activity>
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.smsPhoneVerification" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.autoGeneratedUsername" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.addEmail" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.addPhoneNum" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.logIn" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.setUsername" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.setPassword" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.setBirthday" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.setName" />
        <activity android:name="snapchat.example.snapchat.LoginAndRegisterActivities.Homescreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>